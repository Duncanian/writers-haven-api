{
    "extends": "airbnb-base",
    "env": {
        "node": true,
        "es6": true,
        "jest": true
    },
    "rules": {
        "consistent-return": 0,
        "object-shorthand": 0,
        "no-unused-vars": ["error", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }],
        "max-len": ["error", { "code": 100, "ignoreStrings": true, "ignoreTemplateLiterals": true }]
    }
}
